# even number between from 1 to 10
# all is the list which is declared globaly which is intiallyy empty
def even(num):
 for i in range(1,num):
  if(i%2==0):
   all.append(i)
 return all
#get the number form the user
num=int(input("enter the number"))
all=[]
res=even(num)
print("the reslutant value of even number between 1 to N",res)
