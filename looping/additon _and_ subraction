a=int(input ("enter the first element"))
b=int(input("enter the second element"))
ch=int(input("enter ur choice"))
if(ch==1):
    print("ADDITION)
print(a+b)        
elif(ch==2)
    print("SUBRACTION")
print(a-b)
elif(ch==3)
    print("MUTIPLICATION")
print(a*b)
elif(ch==4)
    print(DIVISON")
print(a/b)
elif(ch==5)
    print("MODULO")
print(a%b)
elif(ch==6)
    print("SQUARE")
print(a**b)
